properties([disableConcurrentBuilds()])

stage('Build') {
    node ('Ruby') {
            echo 'Building...'
            def scmVars = checkout scm
            def branchName = scmVars.GIT_BRANCH
            echo branchName
            sh "ip address"
            sh "${scmVars} > env.txt "
            for (String i : readFile('env.txt').split("\r?\n")) {
                     println i
            }
            
    }    
}
